{
  "name": "ssd-google-sheets-version-control",
  "version": "1.0.0",
  "description": "Version control system for SSD Google Sheets production and staging environments",
  "main": "index.js",
  "scripts": {
    "snapshot": "node scripts/snapshot-all-production.js",
    "snapshot:staging": "node scripts/snapshot-all-staging.js",
    "deploy": "bash scripts/deploy-to-production.sh",
    "rollback": "bash scripts/rollback.sh",
    "registry:update": "node scripts/registry/update-registry.js",
    "registry:validate": "node scripts/registry/validate-registry.js",
    "create-dev": "node scripts/create-dev-sheet.js",
    "test": "node scripts/test-connection.js",
    "credentials:status": "node security/credentials/credential-manager.js status",
    "credentials:check": "node security/credentials/credential-manager.js check-rotations",
    "credentials:rotate": "node security/credentials/credential-manager.js rotate",
    "credentials:audit": "node security/credentials/credential-manager.js audit-report",
    "credentials:alerts": "node security/credentials/credential-manager.js alerts",
    "health-check": "bash scripts/workspace-health-check.sh"
  },
  "keywords": [
    "google-sheets",
    "apps-script",
    "version-control",
    "automation",
    "hipaa"
  ],
  "author": "SSD Practice Management",
  "license": "PRIVATE",
  "dependencies": {
    "@google/clasp": "^2.4.2",
    "dotenv": "^16.3.1",
    "googleapis": "^126.0.1"
  },
  "devDependencies": {
    "eslint": "^8.52.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
